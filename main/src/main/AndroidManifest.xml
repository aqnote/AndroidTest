<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.aqnote.androidtest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk android:minSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.aqnote.androidtest.activity.AboutActivity">
            <intent-filter>
                <action android:name="com.aqnote.androidtest.activity.AboutActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.aqnote.androidtest.activity.SysWebViewActivity">
            <intent-filter>
                <action android:name="com.aqnote.androidtest.activity.SysWebViewActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.aqnote.androidtest.activity.UCWebViewActivity">
            <intent-filter>
                <action android:name="com.aqnote.androidtest.activity.UCWebViewActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>